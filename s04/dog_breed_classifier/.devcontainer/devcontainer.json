{
  "name": "Dog Breed Classifier",
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter"
      ],
      "settings": {
        "python.defaultInterpreterPath": "/usr/bin/python3"
      }
    }
  },
  "forwardPorts": [8080],
  "postCreateCommand": "pip install -r requirements.txt",
  "remoteUser": "root"
}
